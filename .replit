
modules = ["java-graalvm22.3"]

[nix]
channel = "stable-24_05"

[workflows]
runButton = "Spring Boot App"

[[workflows.workflow]]
name = "Spring Boot App"
author = 44520774
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "./mvnw spring-boot:run"

[[ports]]
localPort = 8080
externalPort = 80

[[ports]]
localPort = 35729
externalPort = 3000

[deployment]
run = ["sh", "-c", "mvn spring-boot:run"]
